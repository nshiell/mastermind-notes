{
    "name": "nicholas/mastermind-notes",
    "description": "A place for notes",
    "type": "project",
    "license": "GPL v3",
    "authors": [
        {
            "name": "Nicholas Shiell",
            "email": "nicholas@nshiell.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb": "^1.6",
        "doctrine/mongodb-odm": "^1.2",
        "doctrine/mongodb-odm-bundle": "^3.4",
        "vlucas/phpdotenv": "^2.5@dev",
        "sabre/dav": "~3.2.0",
        "pimple/pimple": "~3.0",
        "phroute/phroute": "dev-master"
    },
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        }
    },
    "autoload": {
        "psr-4": {
            "NShiell\\MastermindNotes\\Entity\\": "Entity/",
            "NShiell\\MastermindNotes\\CalDAV\\Backend\\": "CalDAVBackend/",
            "NShiell\\MastermindNotes\\DAVACL\\PrincipalBackend\\": "DavAclPrincipalBackend/",
            "NShiell\\MastermindNotes\\Dav\\": "Dav/"
        }
    }
}
